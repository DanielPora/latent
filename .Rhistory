group_by(Subject) %>%
summarise(fb_tend = sum(own.cod)/n_obs)
by_subj_lr <- dplyr::filter(data, trial_type == 'LR') %>%
group_by(Subject) %>%
summarise(lr_tend = sum(own.cod)/n_obs)
